      "twilightforest:lich_shield_0": {
        "minecraft:boss": {
          "hud_range": 256,
          "name": "entity.twilightforest:lich.shield.0"
        },
        "minecraft:variant": {
          "value": 2
        },
        "minecraft:environment_sensor": {
          "triggers": [
            {
              "filters": {
                "all_of": [
                  { "test":"has_tag", "value": "twilightforest:has_minion_1" },
                  { "test":"has_tag", "value": "twilightforest:has_minion_2" },
                  { "test":"has_tag", "value": "twilightforest:has_minion_3" },
                  { "test":"has_tag", "value": "twilightforest:has_minion_4" },
                  { "test":"has_tag", "value": "twilightforest:has_minion_5" },
                  { "test":"has_tag", "value": "twilightforest:has_minion_6" },
                  { "test":"has_tag", "value": "twilightforest:has_minion_7" },
                  { "test":"has_tag", "value": "twilightforest:has_minion_8" },
                  { "test":"has_tag", "value": "twilightforest:has_minion_9" }
                ]
              },
              "event": "twilightforest:lich_sword",
              "target": "self"
            },
            {
              "filters": {
                "any_of": [
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_1" },
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_2" },
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_3" },
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_4" },
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_5" },
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_6" },
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_7" },
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_8" },
                  { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minion_9" }
                ]
              },
              "event": "twilightforest:test_minions",
              "target": "self"
            },
            {
              "filters": { "test":"has_tag", "operator": "!=", "value": "twilightforest:has_minions" },
              "event": "twilightforest:need_minions",
              "target": "self"
            }
          ]
        },
        "minecraft:entity_sensor": {
          "event_filters": {
            "test": "is_family",
            "subject": "other",
            "value": "lich_minion"
          },
          "sensor_range": 64,
          "maximum_count": 2,//In fact, there are three rather than two minions at the same time (x
          "event": "twilightforest:need_minions"
        },
        "minecraft:loot": {
          "table": "loot_tables/entities/lich.json"
        },
        "minecraft:experience_reward": {
          "on_death": "217"
        }
      },
      "twilightforest:lich_sword": {
        "minecraft:variant": {
          "value": 1
