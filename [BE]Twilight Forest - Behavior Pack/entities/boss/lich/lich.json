            "execute @s[tag=twilightforest:has_minion_6,tag=!twilightforest:has_minion_7] ~ ~ ~ summon twilightforest:lich_minion ~ ~ ~",
            "execute @s[tag=twilightforest:has_minion_5,tag=!twilightforest:has_minion_6] ~ ~ ~ summon twilightforest:lich_minion ~ ~ ~",
            "execute @s[tag=twilightforest:has_minion_4,tag=!twilightforest:has_minion_5] ~ ~ ~ summon twilightforest:lich_minion ~ ~ ~",
            "execute @s[tag=twilightforest:has_minion_3,tag=!twilightforest:has_minion_4] ~ ~ ~ summon twilightforest:lich_minion ~ ~ ~",
            "execute @s[tag=twilightforest:has_minion_2,tag=!twilightforest:has_minion_3] ~ ~ ~ summon twilightforest:lich_minion ~ ~ ~",
            "execute @s[tag=twilightforest:has_minion_1,tag=!twilightforest:has_minion_2] ~ ~ ~ summon twilightforest:lich_minion ~ ~ ~",
            "execute @s[tag=!twilightforest:has_minion_1] ~ ~ ~ summon twilightforest:lich_minion ~ ~ ~",
            "execute @e[type=twilightforest:lich_minion,r=64,c=1] ~ ~ ~ tag @e[type=twilightforest:lich,r=64] add twilightforest:has_minions",
            "tag @s[tag=twilightforest:has_minion_8,tag=!twilightforest:has_minion_9] add twilightforest:has_minion_9",
            "tag @s[tag=twilightforest:has_minion_7,tag=!twilightforest:has_minion_8] add twilightforest:has_minion_8",
            "tag @s[tag=twilightforest:has_minion_6,tag=!twilightforest:has_minion_7] add twilightforest:has_minion_7",
            "tag @s[tag=twilightforest:has_minion_5,tag=!twilightforest:has_minion_6] add twilightforest:has_minion_6",
            "tag @s[tag=twilightforest:has_minion_4,tag=!twilightforest:has_minion_5] add twilightforest:has_minion_5",
