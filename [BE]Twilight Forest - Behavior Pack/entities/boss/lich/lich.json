              "command": "replaceitem entity @s slot.weapon.mainhand 0 twilightforest:zombie_scepter"
            },
            "trigger": "twilightforest:need_minions"
          },
          {
            "filters": { "test": "has_tag", "subject": "other", "value": "twilightforest:has_shadow_1" },
            "run_command": {
              "command": "tag @s add twilightforest:has_shadow_1"
            }
          },
          {
            "filters": { "test": "has_tag", "subject": "other", "value": "twilightforest:has_shadow_2" },
            "run_command": {
              "command": "tag @s add twilightforest:has_shadow_2"
            }
          }
        ]
      },
      "twilightforest:need_minions": {
        "run_command": {
