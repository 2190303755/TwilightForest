    "events": {
      "minecraft:entity_spawned": {
        "add": {
          "component_groups": [
            "twilightforest:lich_has_shield",
            "twilightforest:lich_shield_5"
          ]
        },
        "run_command": {
          "command": "replaceitem entity @s slot.weapon.mainhand 0 twilightforest:twilight_scepter"
        }
      },
      "twilightforest:is_shadow": {
        "add": {
          "component_groups": [
            "twilightforest:is_shadow"
          ]
        },
        "run_command": {
          "command": [
            "replaceitem entity @s slot.weapon.mainhand 0 twilightforest:twilight_scepter",
            "tag @s add twilightforest:is_shadow"
          ]
        }
      },
      "twilightforest:on_shield_break": {
        "add": {
          "component_groups": [
            "twilightforest:on_shield_break"
          ]
        }
      },
      "twilightforest:setAttackType": {
        "randomize": [
          {
            "weight": 3,
            "add": {
              "component_groups": [
                "twilightforest:ender_pearl"
              ]
            }
          },
          {
            "weight": 1,
            "add": {
              "component_groups": [
                "twilightforest:lich_fireball"
              ]
            }
          }
        ]
      },
      "twilightforest:on_shot": {
        "sequence": [
          {
            "run_command": {
              "command": [
                "execute @s[tag=twilightforest:has_shadow_1,tag=!twilightforest:has_shadow_2,tag=!twilightforest:is_shadow] ~ ~ ~ summon twilightforest:lich ~ ~ ~ twilightforest:is_shadow",
                "execute @s[tag=!twilightforest:has_shadow_1,tag=!twilightforest:is_shadow] ~ ~ ~ summon twilightforest:lich ~ ~ ~ twilightforest:is_shadow",
                "tag @s[tag=twilightforest:has_shadow_1,tag=!twilightforest:has_shadow_2,tag=!twilightforest:is_shadow] add twilightforest:has_shadow_2",
                "tag @s[tag=!twilightforest:has_shadow_1,tag=!twilightforest:is_shadow] add twilightforest:has_shadow_1"
              ]
            }
          },
          /*{
            "run_command": {
              "command": "spreadplayers ~ ~ 0 10 @s"
            }
          },*/
          {
            "trigger": "twilightforest:setAttackType"
          }
        ]
      },
      "twilightforest:getAttackType": {
        "sequence": [
          {
            "filters": {
              "all_of": [
                { "test": "has_component", "operator": "!=", "value": "minecraft:behavior.ranged_attack" },
                { "test": "has_component", "operator": "!=", "value": "minecraft:behavior.melee_attack" }
              ]
            },
            "sequence": [
              {
                "filters": {
                  "all_of": [
                    { "test": "is_variant", "operator": ">=", "value": 2 },
                    { "test": "is_variant", "operator": "<=", "value": 8 }
                  ]
                },
                "add": {
                  "component_groups": [
                    "twilightforest:lich_ranged_attack"
                  ]
                }
              },
              {
                "filters": {
                  "any_of": [
                    { "test": "is_variant", "value": 1 }
                  ]
                },
                "add": {
                  "component_groups": [
                    "twilightforest:lich_melee_attack"
                  ]
                }
              }
            ]
          }
        ]
      },
      "twilightforest:on_target_lost": {
        "remove": {
          "component_groups": [
            "twilightforest:lich_ranged_attack",
            "twilightforest:lich_melee_attack"
          ]
        }
      },
      "minecraft:entity_transformed": {
        "sequence": [
          {
            "filters": { "test": "is_variant", "subject": "other", "value": 7 },
            "add": {
