            "add": {
              "component_groups": [
                "twilightforest:lich_has_shield",
                "twilightforest:lich_shield_2"
              ]
            },
            "run_command": {
              "command": "replaceitem entity @s slot.weapon.mainhand 0 twilightforest:twilight_scepter"
            }
