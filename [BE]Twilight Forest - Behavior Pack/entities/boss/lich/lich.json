            "tag @s[tag=twilightforest:has_minion_2,tag=!twilightforest:has_minion_3] add twilightforest:has_minion_3",
            "tag @s[tag=twilightforest:has_minion_1,tag=!twilightforest:has_minion_2] add twilightforest:has_minion_2",
            "tag @s[tag=!twilightforest:has_minion_1] add twilightforest:has_minion_1"
          ]
        }
      },
      "twilightforest:test_minions": {
        "sequence": [
          {
            "run_command": {
              "command": [
                "tag @s remove twilightforest:has_minions",
                "execute @e[type=twilightforest:lich_minion,r=64,c=1] ~ ~ ~ tag @e[type=twilightforest:lich,r=64] add twilightforest:has_minions"
              ]
            }
          }
        ]
      },
      "twilightforest:lich_sword": {
        "add": {
          "component_groups": [
            "twilightforest:lich_sword"
          ]
        },
        "run_command": {
          "command": "replaceitem entity @s slot.weapon.mainhand 0 golden_sword"
        },
        "trigger": "twilightforest:getAttackType"
      },
      "twilightforest:despawn": {
        "add": {
