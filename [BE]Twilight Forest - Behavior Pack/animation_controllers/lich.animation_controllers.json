{
  "format_version": "1.10.0",
  "animation_controllers": {
    "controller.animation.twilightforest.lich.attack": {
      "states": {
        "default": {
          "transitions": [
            {
              "getAttackType": "query.has_target"
            }
          ],
          "on_entry": [
            "@s twilightforest:on_target_lost"
          ]
        },
        "getAttackType": {
          "transitions": [
            {
              "default": "!query.has_target"
            }
          ],
          "on_entry": [
            "@s twilightforest:getAttackType"
          ]
        }
      }
    }
  }
}